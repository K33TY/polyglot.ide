      This release of the Polyglot IDE is governed by the GNU Lesser General Public License, version 2.1.
   </license>

   <url>
      <update label="Polyglot Updates"
            url="https://www.cs.cornell.edu/projects/polyglot/ide/"/>
      <discovery label="Polyglot project page"
            url="https://www.cs.cornell.edu/projects/polyglot/"/>
   </url>

   <requires>
      <import plugin="polyglot"/>
   </requires>

   <plugin
         id="polyglot.ide"
         download-size="0"
         install-size="0"
         version="@version@"/>
</feature>
